# This is an example of a .env file for a Node.js application.
# Copy this file to .env and fill in the values.
# Make sure to keep this file secret and do not share it publicly.

# Node.js Environment
NODE_ENV=development

# Server Port
# Replace 'your_port_here' with the port number you want to use
PORT=your_port_here

# Database Connection String
# Replace 'your_database_connection_string_here' with your actual connection string
DATABASE_URL=your_database_connection_string_here

# GitHub Access Token
# Replace 'your_github_access_token_here' with your actual token
GITHUB_ACCESS_TOKEN=your_github_access_token_here
GITHUB_TOKEN=your_github_token_here
GITHUB_REPO=your_github_repo_here
GITHUB_USER=your_github_user_here
GITHUB_ORG=your_github_org_here

GITHUB_WEBHOOK_SECRET=your_github_webhook_secret_here
GITHUB_WEBHOOK_URL=your_github_webhook_url_here
GITHUB_WEBHOOK_ID=your_github_webhook_id_here
GITHUB_WEBHOOK_EVENTS=your_github_webhook_events_here

# MongoDB Connection String
# Replace 'your_mongodb_connection_string_here' with your actual connection string
MONGODB_URI=your_mongodb_connection_string_here
# MongoDB Database Name
MONGODB_DB=your_mongodb_database_name_here
# MongoDB Collection Name
MONGODB_COLLECTION=your_mongodb_collection_name_here

# Redis Connection String
# Replace 'your_redis_connection_string_here' with your actual connection string
REDIS_URL=your_redis_connection_string_here
# Redis Database Name
REDIS_DB=your_redis_database_name_here
# Redis Collection Name
REDIS_COLLECTION=your_redis_collection_name_here

# AWS S3 Bucket Name
# Replace 'your_aws_s3_bucket_name_here' with your actual bucket name
AWS_S3_BUCKET=your_aws_s3_bucket_name_here

# AWS Region
# Replace 'your_aws_region_here' with your actual region
AWS_REGION=your_aws_region_here
# AWS Access Keys
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here

# Prometheus and Grafana Credentials
PROMETHEUS_USER=your_prometheus_user_here
PROMETHEUS_PASSWORD=your_prometheus_password_here
GRAFANA_API_KEY=your_grafana_api_key_here

# OpenZeppelin Defender API Keys
DEFENDER_API_KEY=your_defender_api_key_here
DEFENDER_API_SECRET=your_defender_api_secret_here

# Blockchain Node RPC Endpoint
BLOCKCHAIN_RPC_ENDPOINT=your_blockchain_rpc_endpoint_here

# Slack or PagerDuty Alerting
SLACK_WEBHOOK_URL=your_slack_webhook_url_here
PAGERDUTY_API_KEY=your_pagerduty_api_key_here

# Sentry DSN
# Replace 'your_sentry_dsn_here' with your actual DSN
SENTRY_DSN=your_sentry_dsn_here
# Rollbar Access Token
# Replace 'your_rollbar_access_token_here' with your actual access token
ROLLBAR_ACCESS_TOKEN=your_rollbar_access_token_here
# Rollbar Environment
# Replace 'your_rollbar_environment_here' with your actual environment
ROLLBAR_ENVIRONMENT=your_rollbar_environment_here
# Rollbar Version
# Replace 'your_rollbar_version_here' with your actual version
ROLLBAR_VERSION=your_rollbar_version_here
# Rollbar Code Version
# Replace 'your_rollbar_code_version_here' with your actual code version
ROLLBAR_CODE_VERSION=your_rollbar_code_version_here
# Rollbar Host
# Replace 'your_rollbar_host_here' with your actual host
ROLLBAR_HOST=your_rollbar_host_here
# Rollbar Root
# Replace 'your_rollbar_root_here' with your actual root
ROLLBAR_ROOT=your_rollbar_root_here
# Rollbar Code Version
# Replace 'your_rollbar_code_version_here' with your actual code version
ROLLBAR_CODE_VERSION=your_rollbar_code_version_here

# Snyk API Token
SNYK_API_TOKEN=your_snyk_api_token_here

# Web3Auth Credentials
WEB3AUTH_CLIENT_ID=your_web3auth_client_id_here
WEB3AUTH_SECRET_KEY=your_web3auth_secret_key_here

# Tatum API Key
TATUM_API_KEY=your_tatum_api_key_here

# LevitPay API Key
LEVITPAY_API_KEY=your_levitpay_api_key_here

# Stripe API Key
STRIPE_API_KEY=your_stripe_api_key_here

# Other Integrations
OTHER_API_KEY=your_other_api_key_here